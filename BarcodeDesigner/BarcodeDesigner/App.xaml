<Application x:Class="BarcodeDesigner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BarcodeDesigner"
             xmlns:vm="clr-namespace:BarcodeDesigner.ViewModel"
             xmlns:v="clr-namespace:BarcodeDesigner.View.Pages">
    <Application.Resources>
        <DataTemplate DataType="{x:Type vm:HomePageViewModel}">
            <v:HomePage />
        </DataTemplate>

        <DataTemplate DataType="{x:Type vm:SettingsPageViewModel}">
            <v:SettingsPage />
        </DataTemplate>


    </Application.Resources>
</Application>
